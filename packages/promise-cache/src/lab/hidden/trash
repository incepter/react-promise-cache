
export const AsyncStateContext = React.createContext(null)

export function AsyncStateProvider({children}) {
  let ctxValue = React.useMemo(() => {
    let states = new Map()

    return {


      pushState(name, producer) {
        states.set(name, {status: "initial", producer})
      },





      read(name, producer) {
        states.set(name, { producer})
      }

    }
  }, [])
  return (
    <AsyncStateContext.Provider value={ctxValue}>

    </AsyncStateContext.Provider>
  )
}

